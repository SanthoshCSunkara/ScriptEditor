{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["ES2017","DOM"],
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "sourceMap": true,
    "inlineSources": false,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": ["webpack-env"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
